<!DOCTYPE math 
    PUBLIC "-//W3C//DTD MathML 2.0//EN"
           "http://www.w3.org/Math/DTD/mathml2/mathml2.dtd">
<math xmlns="http://www.w3.org/1998/Math/MathML">
    <apply>
        <eq/>
        <ci>
            <msub>
                <mi>UW</mi>
                <mi>FCF, T</mi>
            </msub>
        </ci>
        <apply>
            <divide/>
            <apply>
                <minus/>
                <ci>
                    <msub>
                        <mi>FCF</mi>
                        <mi>T</mi>
                    </msub>
                </ci>
                <apply>
                    <times/>
                    <apply>
                        <times/>
                        <apply>
                            <minus/>
                            <cn>1</cn>
                            <ci>s</ci>
                        </apply>
                        <ci>
                            <msup>
                                <mi>r</mi>
                                <mi>FK</mi>
                            </msup>
                        </ci>
                    </apply>
                    <ci>
                        <msub>
                            <mi>FK</mi>
                            <mi>T</mi>
                        </msub>
                    </ci>
                </apply>
            </apply>
            <ci>
                <msubsup>
                    <mi>rv</mi>
                    <mi>T</mi>
                    <mi>EK</mi>
                </msubsup>
            </ci>
        </apply>
    </apply>
</math>
