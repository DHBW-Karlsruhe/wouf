<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE math 
    PUBLIC "-//W3C//DTD MathML 2.0//EN"
           "http://www.w3.org/Math/DTD/mathml2/mathml2.dtd">
<math xmlns="http://www.w3.org/1998/Math/MathML">
    <apply>
        <eq/>
        <ci>
            <msub>
                <mi>UW</mi>
                <mi>FTE, t</mi>
            </msub>
        </ci>
        <apply>
            <divide/>
            <apply>
                <plus/>
                <ci>
                    <msub>
                        <mi>FTE</mi>
                        <mi>t+1</mi>
                    </msub>
                </ci>
                <ci>
                    <msub>
                        <mi>UW</mi>
                        <mi>FTE, t+1</mi>
                    </msub>
                </ci>
            </apply>
            <apply>
                <plus/>
                <cn>1</cn>
                <ci>
                    <msubsup>
	                    <mi>rv</mi>
	                    <mi>t+1</mi>
	                    <mi>EK</mi>
	                </msubsup>
                </ci>
                </apply>
        </apply>
    </apply>
</math>
